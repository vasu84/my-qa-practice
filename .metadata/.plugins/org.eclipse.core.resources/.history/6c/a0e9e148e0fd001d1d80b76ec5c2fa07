package com.testPractice;

import com.sun.jdi.Method;

public class Profile {
	public static void myDetails() {  /* method ,public means we have to create an object before 
		                  calling an Method. private / static no ned to create an Object.class */
		
		System.out.println("my details is ");
	}
	public int id =256;
	private int age=51; 
	protected String name= "siva";
	public static void main(String[]args) {
		
		
		Profile a = new Profile(); // creating an object , allocating memory 
		myDetails(); // calling method
		
	 System.out.println("Ã¯d    : " + a.id);
		System.out.println( "age   : " + a.age);
		System.out.println( "name  : " + a.name);
				
	}

	 
}
